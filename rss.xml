      style=&quot;position: relative; display: block; margin-left: auto; margin-right: auto;  max-width: 426px;&quot;
    &gt;
      &lt;span
    class=&quot;gatsby-resp-image-background-image&quot;
    style=&quot;padding-bottom: 44.33333333333333%; position: relative; bottom: 0; left: 0; background-image: url(&apos;data:image/png;base64,&apos;); background-size: cover; display: block;&quot;
  &gt;&lt;/span&gt;
  &lt;img
        class=&quot;gatsby-resp-image-image&quot;
        alt=&quot;show dbs&quot;
        title=&quot;show dbs&quot;
        src=&quot;/static/81bd9f6338b9cca716c64efd1acf82e3/531e1/%5Buse-mongodb-1%5Dshow_dbs.png&quot;
        srcset=&quot;/static/81bd9f6338b9cca716c64efd1acf82e3/5a46d/%5Buse-mongodb-1%5Dshow_dbs.png 300w,
/static/81bd9f6338b9cca716c64efd1acf82e3/531e1/%5Buse-mongodb-1%5Dshow_dbs.png 426w&quot;
        sizes=&quot;(max-width: 426px) 100vw, 426px&quot;
